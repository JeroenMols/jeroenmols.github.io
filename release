echo ""
echo "### Cleaning up resources"
cd ../
if [ ! -d jekyll-code ]; then
echo "Aborting due to wrong folder"
exit 1                 
fi
ls | grep -v 'jekyll-code' | xargs rm -rf
cd -
echo ""
echo "### Running jekyll"
bundle exec jekyll build
echo ""
echo "### Copying generated site"
cp -r  _site/* ../
cp CNAME ../
echo ""
echo "### Done"
